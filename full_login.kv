<WindowManager>:
    
    ScreenMain:
        

    ScreenLogin:

    ScreenSignup:


    ScreenSignup2:


    ScreenDash:






















<ScreenDash>
    name: "dash"
    size: root.width, root.height
    Image:
        source: "photos/backgroundf2.png"
        allow_stretch: False
        pos_hint: {"x": -0.3, "y": 0}
        size_hint_y: None
        height: root.height
        size_hint_x: None
        width: root.width
        keep_ratio: False

    MDFloatLayout:
        
        ScreenManager:
            id: scr

            
            
            
            MDScreen:
                name: "home"
                MDLabel:
                    text: "Home"
                    pos_hint: {"center_y": 0.5}
                    halign: "center"
                



            MDScreen:
                name: "form"
                MDLabel:
                    text: "Form"
                    pos_hint: {"center_y": 0.5}
                    halign: "center"



            MDScreen:
                name: "history"
                MDLabel:
                    text: "History"
                    pos_hint: {"center_y": 0.5}
                    halign: "center"



            MDScreen:
                name: "queries"
                MDLabel:
                    text: "Queries"
                    pos_hint: {"center_y": 0.5}
                    halign: "center"
        
        
        NavBar:
            size_hint: .35, .1
            pos_hint: {"center_x": 0.2, "center_y": 0.1}
            elevation: 4
            md_bg_color: 1,1,1,1
            radius: [16]
            MDGridLayout:
                cols: 4
                size_hint_x: 0.9
                spacing: 30
                pos_hint: {"center_x": 0.45, "center_y": 0.4}

                
                
                MDIconButton:
                    id: home_icon
                    icon: "home"
                    ripple_scale: 0
                    user_font_size: "30sp"
                    theme_text_color: "Custom"
                    text_color: 0,0,0,1
                    on_release: scr.current = "home"


                MDIconButton:
                    id: form_icon
                    icon: "form-select"
                    ripple_scale: 0
                    user_font_size: "30sp"
                    theme_text_color: "Custom"
                    text_color: 0,0,0,1
                    on_release: scr.current = "form"


                MDIconButton:
                    id: history_icon
                    icon: "history"
                    ripple_scale: 0
                    user_font_size: "30sp"
                    theme_text_color: "Custom"
                    text_color: 0,0,0,1
                    on_release: scr.current = "history"

                MDIconButton:
                    id: queries_icon
                    icon: "database-search"
                    ripple_scale: 0
                    user_font_size: "30sp"
                    theme_text_color: "Custom"
                    text_color: 0,0,0,1
                    on_release: scr.current = "queries"






































